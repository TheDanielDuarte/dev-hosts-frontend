{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ],
      "urls": [
        "https://fonts.googleapis.com/css?family=Montserrat+Alternates"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "api-data",
      "urls": [
        "https://devhosts.herokuapp.com/api/servers",
        "https://devhosts.herokuapp.com/api/storage-center",
        "https://devhosts.herokuapp.com/api/services"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "2d",
        "strategy": "performance"
      }
    }
  ]
}